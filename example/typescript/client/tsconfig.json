{	
	"compileOnSave": true,
    "compilerOptions": {
		"target": "es2016",
        "lib": [
            "dom",
            "es2016"
        ],
        "module":"commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "preserveConstEnums": true,
		"sourceMap": false,
        "strictNullChecks": true,
		"outDir": "build",
        "baseUrl": ".",
        "paths": {
            "*": [
                "src/*",
                "generated/*"
            ]
        }
	}
}